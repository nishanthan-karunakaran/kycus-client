/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["@angular/localize"],
    "paths": {
      "@assets/*": ["/src/app/assets/*"],
      "@features/*": ["/src/app/features/*"],
      "@src/*": ["*"],
      "@core": ["/src/app/core/*"]
    }
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"]
}
